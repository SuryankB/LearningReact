{
  "manifest_version": 3,
  "name": "Password Generator",
  "version": "1.0",
  "description": "Generate secure passwords instantly",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "clipboardWrite"]
}
